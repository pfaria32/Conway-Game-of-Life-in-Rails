<div style="text-align: center;"><button id="go">Go!</button><button id="stop">stop!</button></div>
<div class="container">
  <table style="width: 100%">
    <% 51.times do  %>
      <tr>
        <% 51.times do %>
            <td class="cell_dead" id=<%= "#{@x},#{@y}" %>></td>
            <% @x += 1 %>
            <% end %>
      <tr>
      <% @x = 0 %>
      <% @y -= 1 %>
    <% end %>
  </table>
</div>
<%= javascript_pack_tag 'cell_dead' %>
<%= javascript_pack_tag 'cell_alive' %>
<%= javascript_pack_tag 'cell_behavior' %>
